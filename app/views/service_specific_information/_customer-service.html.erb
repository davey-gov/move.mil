<%- if contact.ppm_contact_info? -%>
  <div class="svc-spec-links">
    <header>Personally Procured Moves (PPM): Submit or Check Status</header>
    <section>
      <table>
        <tbody>
          <%- if contact.ppm_website -%>
            <tr>
              <th scope="row">Website:</th>
              <td><%= link_to contact.ppm_website, contact.ppm_website %></td>
            </tr>
          <%- end -%>
          <%- if contact.ppm_tel_comm -%>
            <tr>
              <th scope="row">Comm:</th>
              <td><%= contact.ppm_tel_comm %></td>
            </tr>
          <%- end -%>
        </tbody>
      </table>
    </secton>
  </div>
<%- end -%>
<%- if policies.any? -%>
  <div class="svc-spec-links">
    <header>Supplemental Policies</header>
    <section>
      <ul class="usa-unstyled-list">
        <%- policies.each do |policy| -%>
          <li><%= link_to policy[:name], policy[:url], class: 'usa-external_link' %></li>
        <%- end -%>
      </ul>
    </section>
  </div>
<%- end -%>
